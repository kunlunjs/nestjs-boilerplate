{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack": "tsc -p tsconfig.build.json",
    "build:webpack:dev": "nest build --watch --webpack --webpackPath webpack-hmr.config.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:nodemon": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "generate-typings": "ts-node src/modules/graphql-schema-first/generate-typings.ts",
    "eslint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prepare": "husky install",
    "husky:pre-commit": "lint-staged",
    "husky:prepare-commit-msg": "devmoji -e --lint"
  },
  "keywords": ["nestjs", "typeorm", "graphql", "mongoose", "prisma"],
  "author": "zhuyudong@aliyun.com",
  "license": "ISC",
